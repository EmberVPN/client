{
	"extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
	"include": [
		"electron.vite.config.*",
		"src/main/*",
		"src/preload/*",
		"src/ember.d.ts",
	],
	"compilerOptions": {
		"composite": true,
		"noUnusedParameters": false,
		"noUnusedLocals": false,
		"types": [
			"electron-vite/node"
		]
	}
}